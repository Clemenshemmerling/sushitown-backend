{
  "name": "sushitown-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "migrate": "dotenv -e .env -- sequelize-cli db:migrate",
    "migrate:undo": "dotenv -e .env -- sequelize-cli db:migrate:undo",
    "migrate:undo:all": "dotenv -e .env -- sequelize-cli db:migrate:undo:all",
    "predev": "dotenv -e .env -- sequelize-cli db:migrate",
    "dev": "dotenv -e .env -- nodemon --watch src src/server.js --host 0.0.0.0",
    "prestart": "dotenv -e .env -- sequelize-cli db:migrate",
    "start": "dotenv -e .env -- node src/server.js",
    "test": "nodemon src/server.js"
  },
  "author": "Clemens Hemmerling",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "dotenv-cli": "^10.0.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  }
}
